{
  "name": "contactmanager",
  "version": "1.0.0",
  "description": "MERN contact manager app",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "server": "clear && nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "clear && concurrently \"nodemon backend/server\" \"npm run client\"",
    "data:import": "clear && node backend/seeder",
    "data:destroy": "clear && node backend/seeder -d"
  },
  "author": "Rick Walker",
  "license": "MIT",
  "repository": "https://github.com/quobod/contactsmanager",
  "dependencies": {
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "nanoid": "^3.1.16",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.14",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0"
  }
}
