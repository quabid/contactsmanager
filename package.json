{
  "name": "contactmanager",
  "version": "1.0.0",
  "description": "MERN contact manager app",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "clear && nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "clear && concurrently \"nodemon backend/server\" \"npm run client\"",
    "data:import": "clear && node backend/seeder",
    "data:destroy": "clear && node backend/seeder -d"
  },
  "author": "Rick Walker",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "nanoid": "^3.1.12"
  },
  "devDependencies": {
    "bunyan": "^1.8.14",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0"
  }
}
